/* Error Message start */

.errorExplanation {
    margin-left:210px;
    padding:15px;
}

select.error, textarea.error {
    border:1px dotted #ff6f0e;
}

font {
    color:#ff6f0e !important;
}


.fieldWithErrors input,.fieldWithErrors textarea, .fieldWithErrors select{
    border:1px dotted #ff6f0e;
}

.errorExplanation ul, ul.error_ul {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    padding: 0px 0px 0px 0px;
    margin: 0px 0px 0px 0px;
    text-align: left;
}

.errorExplanation ul li, ul.error_ul li {
    list-style: none;
    background: url(/images/notification01.png) no-repeat 0px -24px;
    text-align: left;
    font-size: 11px;
    line-height: 12px;
    font-weight: bold;
    color: #ff6f0e;
    line-height: 18px;
    padding: 0px 0px 4px 26px;
    margin: 0px 0px 0px 0px;
}
/* Error Message End */
/* Succes Message start */
ul.success_ul {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    padding: 0px 0px 0px 0px;
    margin: 0px 0px 0px 0px;
    text-align: left;
}

ul.success_ul li {
    list-style: none;
    background: url(/images/notification01.png) no-repeat 0px 0px;
    text-align: left;
    font-size: 11px;
    line-height: 12px;
    font-weight: bold;
    color: #81a13d;
    line-height: 18px;
    padding: 0px 0px 4px 26px;
    margin: 0px 0px 0px 0px;
}

ul.cat_success_ul {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    padding: 0px 0px 0px 0px;
    margin: 0px 0px 0px 0px;
    text-align: left;
}

ul.cat_success_ul li {
    list-style: none;
    background: url(/images/icon_errorgreen.gif) no-repeat;
    text-align: left;
    font-size: 11px;
    line-height: 12px;
    font-weight: bold;
    color: #81a13d;
    line-height: 18px;
    padding: 0px 0px 4px 26px;
    margin: 0px 0px 0px 4px;
}


/* Succes Message End */
.message {
    padding: 90px 0px 0px 200px;
    font-weight: bold;
}
/* notice message */
.notice-wrap {
    position: fixed;
    top: 160px;
    left: auto;
    z-index: 9999;

    padding: 5px;

}

* html .notice-wrap {
    position: absolute;

}

#notice-item {
    width: 250px;

    text-align: center;
    background: #000;
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    color: #eee;
    padding: 16px 6px 16px 6px;
    *padding: 16px 6px 16px 6px;
    font-size: 11px;
    font-weight: bold;
    border: 2px solid #742D00;
    display: block;
    position: relative;
    margin: 0 0 12px 0;
}
