a.ovalbutton {background: transparent url('../images/oval-orange-left.gif') no-repeat top left !important; display: block; float: left; font: normal 12px Arial; line-height: 16px; height: 18px; padding-left: 11px; text-decoration: none;}
a:link.ovalbutton, a:visited.ovalbutton, a:active.ovalbutton{color: #000; font-size:11px; font-weight:bold;}
a.ovalbutton span{background: transparent url('../images/oval-orange-right.gif') no-repeat top right; display: block; padding: 0px 11px 2px 0;}
a.ovalbutton:hover{background-position: left -18px; color:#3c5605; font-size:11px; text-decoration:none !important;}
a.ovalbutton:hover span{background-position:  right -18px; color:#3c5605; font-size:11px; text-decoration:none !important;}
a.ovalbutton:active{background-position: bottom left;}
a.ovalbutton:active span{background-position: bottom right;}
