<!-- Profile File Include --><%#= javascript_include_tag "Profile" %>
<div class="clear">
</div>
<div class="content">
    <!-- Left Col Start -->
    <div class="c1">
        <!-- Set Priority File Include -->
        <%#= javascript_include_tag "SetPriority"%>
        <div class="clear">
        </div>
        <!-- Blue Box start -->
      <!-- Blue Box start -->
          <div class="Blue4pxbox"><code>$(this).corner();</code>
                <div class="whiteBoxContent">
                <!-- Design Central Start -->
                	<!-- Section Head start -->
					<div class="pageheading">
                        <div class="c1 head14">Active Contributors</div>
                        <div class="c5 total">
                            <div id="search_box">
                                <%@type="learnings/active_contributors"%>
                                <%=render :partial=>"/designs/search"%>
                             <!--
                                <form action="/" method="post" id="search_form">
                                    <input type="text" class="swap_value" value="Search" id="s">
                                    <input type="image" height="20" width="18" title="Search" alt="Search" id="go" src="../images/btn_search_box.gif">
                                </form>
                             -->
                            </div>
                      </div>
                    </div>
                    <p class="clear brdBot01"></p>
                   <div class="pageheading">
                        <p class="c1 total"><a>Total Acitve Contributors: <%= @active_participants.size%></a></p>
                        <p class="c3"><span id="pagination" class="pagination01"></span></p>
                    </div>
                    <p class="clear brdBot01"></p>
                	<!-- Section Head End -->

                <!-- Two col comment start -->
               <%= render :partial => "/discussions/active_participant", :collection => @active_participants%>
               <%#=render :partial=>"/users/contributors.html.erb"%>

                <p class="clear"></p>


                <!-- Design Central End -->

                </div>
            </div>

        <!-- Blue Box end -->
        <div class="spacer">
        </div>
    </div>
    <!-- Left Col End -->
    <!-- Right Col Start -->
    <div class="c2 rightcol" id="rightcol">
        <%= render :partial => "right_col" %>
    </div>
    <!-- Right Col End -->
</div>

<%if @no_of_pages >= 2%>
  <script type="text/javascript">
     $(document).ready(function(){
    $(function(){
      $("#pagination").paginate({ 
        <%if @search_text%>
          href : '/learnings/active_contributors?search=<%=@search_text%>&page=',                 
        <%else%>
          href : '/learnings/active_contributors?page=',   
        <%end%>
        count: <%=@no_of_pages%>,
        start: <%=params[:page] || 1%>,
        display: 5,
        border: false,
        text_color: '',
        background_color: '',
        text_hover_color: '',
        background_hover_color: ''
      });
    });
     });
    </script>
<%end%>