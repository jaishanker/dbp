<!-- Profile File Include --><%#= javascript_include_tag "Profile" %>
<div class="clear">
</div>
<!-- Container Start -->
<div class="content">
    <!-- Left Col Start -->
    <div class="c1">
        <!-- Set Priority File Include --><%#= javascript_include_tag "SetPriority" %>
        <div class="clear">
        </div>
        <!-- Blue Box start -->
        <div class="Blue4pxbox">
            <code>
                $(this).corner();
            </code>
            <div class="whiteBoxContent">
                <!-- Design Central Start --><!-- Section Head start -->
                <div class="pageheading">
                    <div class="c1 head14">
                        My Favorite Designs
                    </div>
                    <div class="c5 total">
                        <div id="search_box">
                            <form action="/" method="post" id="search_form">
                                <input type="text" class="swap_value" value="Search" id="s"><input type="image" height="20" width="18" title="Search" alt="Search" id="go" src="../images/btn_search_box.gif">
                            </form>
                        </div>
                    </div>
                </div>
                <p class="clear brdBot01">
                </p>
                <div class="pageheading">
                    <p class="c1 total">
                        <a href="#">Total Designs : <%= @designs.length %></a>
                        | <a href="#" class="archive">Select Archive</a>
                    </p>
                    <p class="c3">
                        <span id="pagination" class="pagination01"></span>
                    </p>
                </div>
                <p class="clear brdBot01">
                </p>
                <!-- Section Head End -->
                <!-- Design list -->
                <% unless @designs.length == 0 %>
                <% @designs.each do |d| %>
                <%= render :partial => "design",:locals => {:d => d,:same_design => false},:inline=>true %>
                <% end %>
                <% else %>
                No designs found<% end %>
                <!-- Design Central End -->
            </div>
        </div>
        <!-- Blue Box end -->
        <div class="spacer">
        </div>
    </div>
    <!-- Left Col End -->
    <!-- Right Col Start -->
    <div class="c2 rightcol">
        <!-- My Page Links Include -->
        <%= render :partial => 'right_col'%>
        <%#= javascript_include_tag "MyPage_links" %>
    </div>
    <!-- Right Col End -->
</div>
